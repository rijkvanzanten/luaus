<%- include partials/logo %>

<% if (Object.keys(games).length > 0) { %>
  <ul>
    <% Object.keys(games).forEach(function (gameID) { %>
    <li><a href="<%= gameID %>"><%= gameID %> <%= Object.keys(games[gameID].players).length %> / 12</a></li>
    <% }); %>
  </ul>
<% } else { %>
  <p>No Luaus found</p>
<% } %>

<form method="post" action="/">
  <button>Create Luau</button>
</form>

